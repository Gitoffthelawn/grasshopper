{
    "manifest_version": 2,
    "name": "Grasshopper",
    "version": "1.7",
    "description": "Go to your favorites URLs quickly",
    "permissions": [
        "history",
        "storage"
    ],
    "icons": {
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "icon128.png"
        },
        "default_title": "Grasshopper",
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_browser_action": {

        }
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "grasshopper@mp.com",
            "strict_min_version": "90.0"
        }
    }
}