{
    "manifest_version": 2,
    "name": "Grasshopper",
    "version": "204",
    "description": "Awesome Tab Manager",
    "permissions": [
        "tabs",
        "sessions",
        "history",
        "bookmarks"
    ],
    "icons": {
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "img/icon128.png"
        },
        "default_title": "Grasshopper",
        "default_popup": "popup.html"
    },
    "sidebar_action": {
        "default_title": "Grasshopper",
        "default_panel": "popup.html",
        "default_icon": "img/icon128.png"
    }
}