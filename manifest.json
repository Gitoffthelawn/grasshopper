{
    "manifest_version": 2,
    "name": "Grasshopper",
    "version": "714",
    "description": "Godspeed You Tab Emperor",
    "permissions": [
        "tabs",
        "sessions",
        "history",
        "bookmarks"
    ],
    "icons": {
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "img/icon128.png"
        },
        "default_title": "Grasshopper",
        "default_popup": "main.html"
    },
    "sidebar_action": {
        "default_title": "Grasshopper",
        "default_panel": "main.html",
        "default_icon": "img/icon128.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Space"
            },
            "description": "Toggle the popup"
        },
        "_execute_sidebar_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+Space"
            },
            "description": "Toggle the sidebar"
        }
    }
}